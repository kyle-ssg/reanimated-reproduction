{
  "name": "ssg-webpack-es6-boilerplate",
  "version": "0.7.1",
  "description": "SSG React Boilerplate with Flux",
  "repository": {
    "git": "https://git.solidstategroup.com/solidstategroup/ssg-frontend-boilerplate"
  },
  "license": "MIT",
  "author": "SSG",
  "scripts": {
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "build:html": "node ./bin/buildHtml.js",
    "clean:build": "node ./bin/clean.js createdir",
    "jscs": "jscs src/js/",
    "lint": "eslint config src/js/** --ext .js",
    "fix": "eslint --fix config src/js/** --ext .js",
    "prestart": "node ./bin/clean.js",
    "start": "node ./bin/kill.js & webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress & node ./bin/ngrok.js",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "fetchify": "0.0.2",
    "firebase": "3.2.0",
    "flux-react-dispatcher": "1.2.5",
    "history": "3.0.0",
    "lodash": "4.13.1",
    "moment": "2.14.1",
    "rc-switch": "1.4.2",
    "react": "^15.3.0",
    "react-addons-shallow-compare": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-maskedinput": "3.2.3",
    "react-router": "2.6.0",
    "react-select": "^1.0.0-beta14",
    "react-tooltip": "3.1.2",
    "react-virtualized": "^7.0.0",
    "react-virtualized-select": "1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.5.0",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-watch": "4.3.9",
    "jscs": "3.0.7",
    "jsdom": "9.4.1",
    "mocha": "2.5.3",
    "ngrok": "2.2.2",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "ps-node": "0.1.2",
    "react-addons-test-utils": "15.2.1",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
