{
  "name": "ssg-frontend-boilerplate",
  "version": "0.1.1",
  "description": "Node Express Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm run bundle && NODE_ENV=production node ./server",
    "dev": "nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "android": "cd ./mobile && npm run android",
    "ios": "cd ./mobile && npm run ios",
    "test": "nodemon --exec mocha ./server/tests/",
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js"
  },
  "dependencies": {
    "animate.css": "3.5.2",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "2.2.5",
    "colors": "1.1.2",
    "create-react-class": "15.5.3",
    "firebase": "3.2.0",
    "flux-react-dispatcher": "1.2.5",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-watch": "4.3.11",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "ps-node": "0.1.4",
    "rc-switch": "1.4.2",
    "react-froala-wysiwyg": "2.7.0",
    "react-maskedinput": "3.3.4",
    "react-router": "4.2.0",
    "react-select": "1.0.0-rc.3",
    "react-slider": "0.7.0",
    "react-sortable-hoc": "0.0.6",
    "react-table": "6.5.3",
    "react-tooltip": "3.1.2",
    "react-virtualized": "9.7.5",
    "simple-firebase-auth": "^0.0.6",
    "ssg-node-express": "4.14.2",
    "ssg-react": "15.6.1",
    "ssg-webpack": "2.2.1",
    "webpack-bundle-size-analyzer": "2.7.0"
  }
}
