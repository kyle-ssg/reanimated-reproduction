{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm run env_script",
    "now-build": "npm run build",
    "analyze": "cross-env BUNDLE_ANALYZE=true next build ",
    "dev": "npm run storybook & node server.js",
    "dev:debug": "node server.js --inspect",
    "env_script": "sh ./bin/env.sh",
    "build": "next build ",
    "static": "npm run build && npm run export",
    "export": "next export ",
    "storybook": "start-storybook --ci -p 4000 --quiet",
    "start": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --quiet || exit 0",
    "test:e2e": "cross-env E2E=true next build && cross-env NODE_ENV=production E2E=true nightwatch ./tests/e2e/index.e2e.js -c ./tests/e2e/nightwatch.conf.js",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts tests/mocha.opts",
    "test:unit:debug": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --debug-brk --opts tests/mocha.opts",
    "test:unit:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --watch --opts tests/mocha.opts \\\"./common/**/*.test.js\\\"",
    "test:pact": "cross-env NODE_ENV=pactTest node_modules/mocha/bin/mocha --timeout 50000 --require @babel/register \"**/*.pact.js\"",
    "test:pact:server": "cross-env MOCK_SERVER=true node_modules/.bin/nodemon --watch tests --exec node ./tests/pact/helpers/pact.mock-server.js",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "cacheable-response": "^1.9.1",
    "chromedriver": "^76.0.0",
    "classnames": "2.2.6",
    "colors": "1.3.3",
    "cookie": "0.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.0.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "immer": "^6.0.2",
    "inputmask-core": "^2.2.0",
    "ionicons": "^4.6.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lodash": "4.17.15",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "next": "^9.3.4",
    "next-fonts": "^1.0.3",
    "next-images": "^1.4.0",
    "next-offline": "^5.0.0",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "polyfill-react-native": "1.0.5",
    "prop-types": "^15.7.2",
    "rc-switch": "1.9.0",
    "react": "^16.13.1",
    "react-autocomplete": "^1.8.1",
    "react-autolinker": "^1.0.7",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-localization": "^1.0.15",
    "react-redux": "^7.2.0",
    "reactstrap": "8.0.1",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.8.7",
    "@next/bundle-analyzer": "^9.3.4",
    "@pact-foundation/pact": "9.1.0",
    "@pact-foundation/pact-node": "^9.0.2",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@welldone-software/why-did-you-render": "^4.0.7",
    "@zeit/next-source-maps": "0.0.3",
    "array-find-index": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "5.2.0",
    "dotenv": "6.2.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "nodemon": "^2.0.3",
    "prettier": "1.15.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.26.3",
    "storybook-addon-smart-knobs": "^6.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "3.7.3"
  }
}
