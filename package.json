{
  "name": "shared-react",
  "version": "0.1.0",
  "description": "An example cross platform application",
  "main": "index.js",
  "repository": "https://github.com/SolidStateGroup/shared-react",
  "license": "MIT",
  "scripts": {
    "analyse": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.analyse.js",
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js",
    "dev": "npm run lint:watch & npm run env_dev && node_modules/.bin/nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "env_dev": "cross-env ENV=dev node ./bin/env.js",
    "env_prod": "cross-env ENV=prod node ./bin/env.js",
    "lint": "eslint 'web/**/*.js' 'tests/**/*.js' 'common/**/*.js'",
    "lint:fix": "eslint --fix 'web/**/*.js' 'common/**/*.js'",
    "lint:watch": "esw -w --changed 'web/**/*.js'",
    "start": "npm run env_prod && npm run bundle && NODE_ENV=production node ./server",
    "test:e2e": "cross-env E2E=true nightwatch ./tests/e2e/index.e2e.js",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:unit:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --watch --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:pact": "cross-env NODE_ENV=pactTest mocha --opts ./tests/mocha.opts \"**/*.pact.js\"",
    "test:pact:server": "cross-env MOCK_SERVER=true node_modules/.bin/nodemon --watch tests --exec node ./tests/pact/helpers/pact.mock-server.js",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js",
    "test:coverage": "istanbul cover ./web/main.js"
  },
  "engines": {
    "node": "9.10.1",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@pact-foundation/pact": "^7.2.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "bootstrap": "4.1.3",
    "bullet-train-client": "0.0.42",
    "chai": "4.2.0",
    "chromedriver": "2.42.0",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "0.1.19",
    "color": "^3.1.0",
    "colors": "1.3.2",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.0",
    "docz": "^0.12.5",
    "dotenv": "^6.1.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fbjs": "0.8.17",
    "fetch-mock": "^7.2.5",
    "file-loader": "2.0.0",
    "flux-react-dispatcher": "1.2.5",
    "history": "^4.7.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.3.1",
    "js-base64": "2.4.9",
    "js-cookie": "^2.2.0",
    "lodash": "4.17.11",
    "mocha": "^5.2.0",
    "moment": "2.22.2",
    "nightwatch": "1.0.11",
    "node-fetch": "2.2.0",
    "node-sass": "4.11.0",
    "nodemon": "1.18.7",
    "nyc": "^13.1.0",
    "polyfill-react-native": "1.0.5",
    "postcss-loader": "2.1.2",
    "prettier": "^1.15.2",
    "promise-polyfill": "7.1.2",
    "prop-types": "15.6.2",
    "rc-slider": "^8.6.7",
    "rc-switch": "1.8.0",
    "react": "16.6.3",
    "react-datepicker": "^2.0.0",
    "react-dom": "16.6.3",
    "react-hot-loader": "4.3.11",
    "react-maskedinput": "4.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-tooltip": "3.9.2",
    "react-virtualized": "9.20.1",
    "sass-loader": "https://github.com/SolidStateGroup/sass-loader",
    "selenium-server": "3.14.0",
    "sinon": "^7.1.1",
    "slackbots": "^1.2.0",
    "ssg-node-express": "4.16.3",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {}
}
