{
  "name": "shared-react",
  "version": "0.1.0",
  "description": "An example cross platform application",
  "main": "index.js",
  "scripts": {
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js",
    "dev": "npm run env_dev && node_modules/.bin/nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "env_dev": "ENV=dev node ./bin/env.js",
    "env_prod": "ENV=prod node ./bin/env.js",
    "html_bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.html.prod.js",
    "lint": "eslint web/**/*.js tests/**/*.js common/**/*.js",
    "lint:fix": "eslint --fix web/**/*.js",
    "start": "npm run env_prod && npm run bundle && NODE_ENV=production node ./server",
    "test": "nightwatch ./tests/index.test.js"
  },
  "engines": {
    "node": "9.10.1",
    "npm": "5.6.0"
  },
  "dependencies": {
    "bootstrap": "4.1.1",
    "classnames": "^2.2.5",
    "colors": "^1.3.2",
    "fbjs": "^0.8.17",
    "flux-react-dispatcher": "1.2.5",
    "ionicons": "^4.4.3",
    "js-base64": "^2.4.9",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.8.3",
    "polyfill-react-native": "^1.0.5",
    "promise-polyfill": "^7.1.2",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.2",
    "rc-switch": "^1.6.0",
    "react": "^16.5.0",
    "react-addons-shallow-compare": "^15.3.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-maskedinput": "^4.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-slider": "^0.11.2",
    "react-tooltip": "^3.8.1",
    "react-virtualized": "9.7.5",
    "ssg-node-express": "4.16.3",
    "whatwg-fetch": "^3.0.0"
