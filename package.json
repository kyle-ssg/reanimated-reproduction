{
  "name": "ssg-webpack-es6-boilerplate",
  "version": "0.7.1",
  "description": "SSG React Boilerplate with Flux",
  "repository": {
    "git": "https://git.solidstategroup.com/solidstategroup/ssg-frontend-boilerplate"
  },
  "license": "MIT",
  "author": "SSG",
  "scripts": {
    "start": "npm run env:dev && npm run webpack:dev",
    "watch": "npm run env:dev && npm run webpack:watch",
    "prod": "npm run env:dev && npm run webpack:prod",
    "android": "cd ./mobile && npm run android",
    "ios": "cd ./mobile && npm run ios",
    "prebuild": "npm run clean:build && npm run build:html",
    "build:html": "node ./bin/buildHtml.js",
    "clean:build": "node ./bin/clean.js createdir",
    "jscs": "jscs src/js/",
    "lint": "eslint config src/js/** --ext .js",
    "fix": "eslint --fix config src/js/** --ext .js",
    "prestart": "node ./bin/clean.js",
    "env:prod": "node ./bin/env.js prod",
    "env:dev": "node ./bin/env.js dev",
    "webpack:dev": "node & webpack-dashboard -m -- webpack-dev-server --watch --hot --progress --inline --config ./config/webpack.config.dev.js",
    "webpack:prod": "node & webpack-dashboard -m -- webpack --progress --config ./config/webpack.config.prod.js",
    "webpack:watch": "node & webpack-dashboard -m -- webpack --watch --config --progress ./config/webpack.config.watch.js"
  },
  "dependencies": {
    "analytics-node": "2.1.0",
    "bootstrap": "4.0.0-alpha.3",
    "classnames": "2.2.5",
    "fetchify": "0.0.2",
    "firebase": "3.2.0",
    "flux-react-dispatcher": "1.2.5",
    "history": "3.0.0",
    "json-loader": "0.5.4",
    "lodash.findindex": "4.5.1",
    "lodash.foreach": "4.4.1",
    "lodash.indexof": "4.0.4",
    "lodash.merge": "4.5.1",
    "lodash.orderby": "4.5.1",
    "lodash.range": "3.1.7",
    "moment": "2.14.1",
    "rc-switch": "1.4.2",
    "react": "^15.3.0",
    "react-addons-shallow-compare": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-maskedinput": "3.2.3",
    "react-native-fabric-digits": "1.0.21",
    "react-router": "2.6.0",
    "react-slider": "0.7.0",
    "react-sortable-hoc": "0.0.6",
    "react-tooltip": "3.1.2",
    "react-virtualized": "^7.0.0",
    "react-virtualized-infinite-scroll": "^0.0.4",
    "simple-firebase-auth": "^0.0.2"
  },
  "devDependencies": {
    "babel-preset-react": "6.11.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader": "0.4.3",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-1": "6.5.0",
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "ngrok": "2.2.2",
    "react-hot-loader": "1.3.0",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dashboard": "https://github.com/kyle-ssg/webpack-dashboard.git#master",
    "webpack-dev-server": "1.14.1"
  }
}
