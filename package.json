    "static": "npm run build && npm run export",
    "export": "next export ",
    "start": "cross-env NODE_ENV=production cross-env NODE_ENV=production next start",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint . --ext .tsx,.ts --fix --quiet",
    "typecheck": "tsc",
    "test:e2e:prod": "echo implement testcafe",
    "storybook": "start-storybook -p 4000 -s ./public",
    "build-storybook": "build-storybook"
  },
