{
  "name": "shared-react",
  "version": "0.1.0",
  "description": "An example cross platform application",
  "main": "index.js",
  "repository": "https://github.com/SolidStateGroup/shared-react",
  "license": "MIT",
  "scripts": {
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js",
    "dev": "npm run lint:watch & npm run env_dev && node_modules/.bin/nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "env_dev": "ENV=dev node ./bin/env.js",
    "env_prod": "ENV=prod node ./bin/env.js",
    "html_bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.html.prod.js",
    "lint": "eslint web/**/*.js tests/**/*.js common/**/*.js",
    "lint:fix": "eslint --fix web/**/*.js common/**/*.js",
    "lint:watch": "esw -w -f table --changed web/**/*.js common/**/*.js",
    "start": "npm run env_prod && npm run bundle && NODE_ENV=production node ./server",
    "test": "nightwatch ./tests/index.test.js"
  },
  "engines": {
    "node": "9.10.1",
    "npm": "5.6.0"
  },
  "dependencies": {
    "bootstrap": "4.1.3",
    "classnames": "2.2.6",
    "colors": "1.3.2",
    "fbjs": "0.8.17",
    "flux-react-dispatcher": "1.2.5",
    "ionicons": "4.4.4",
    "js-base64": "2.4.9",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "node-fetch": "2.2.0",
    "node-sass": "4.9.3",
    "polyfill-react-native": "1.0.5",
    "promise-polyfill": "7.1.2",
    "prop-types": "15.6.2",
    "rc-slider": "8.6.3",
    "rc-switch": "1.8.0",
    "react": "16.5.2",
    "react-addons-shallow-compare": "15.6.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-maskedinput": "4.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-slider": "0.11.2",
    "react-tooltip": "3.8.4",
    "react-virtualized": "9.20.1",
    "ssg-node-express": "4.16.3",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
