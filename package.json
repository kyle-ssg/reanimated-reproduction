{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm run env_script",
    "now-build": "npm run build",
    "analyze": "cross-env BUNDLE_ANALYZE=true next build ",
    "dev": "next",
    "env_script": "sh .bin/env.sh",
    "postinstall": "patch-package && husky install",
    "build": "next build ",
    "static": "npm run build && npm run export",
    "export": "next export ",
    "start": "cross-env NODE_ENV=production cross-env NODE_ENV=production next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --quiet || exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cache": "jest --clearCache",
    "writeEnv": "node ./.bin/write-env",
    "test:e2e": "npx nightwatch -e chrome",
    "test:e2e:headless": "npx nightwatch -e chromeHeadless",
    "test:e2e:prod": "cross-env NODE_ENV=production E2E=true npm run writeEnv && npm run build && npx nightwatch -e chromeProd",
    "test:e2e:prodLocal": "npx nightwatch -e chromeProd",
    "test:pact": "cross-env NODE_ENV=pactTest node_modules/mocha/bin/mocha --timeout 50000 --require @babel/register \"**/*.pact.js\"",
    "test:pact:server": "cross-env MOCK_SERVER=true node_modules/.bin/nodemon --watch tests --exec node ./tests/pact/helpers/pact.mock-server.js",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js",
    "storybook": "start-storybook -p 4000 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "4.5.11",
    "aws-amplify": "3.3.20",
    "bootstrap": "4.6.0",
    "classnames": "2.3.1",
    "cookie": "0.4.1",
    "date-fns": "2.12.0",
    "e2e-proxy": "1.7.0",
    "fbjs": "^3.0.0",
    "formik": "2.2.9",
    "immer": "9.0.6",
    "isomorphic-unfetch": "3.0.0",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "moment": "2.24.0",
    "next": "12.0.2",
    "next-compose-plugins": "2.2.1",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "5.0.0",
    "next-transpile-modules": "^8.0.0",
    "rc-switch": "1.9.2",
    "react": "17.0.2",
    "react-datepicker": "2.14.1",
    "react-dom": "17.0.2",
    "react-localization": "1.0.15",
    "react-native-globals": "^0.64.0",
    "react-native-reanimated": "2.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "^12.1.1",
    "react-native-web": "^0.17.1",
    "react-native-web-lottie": "^1.4.4",
    "react-redux": "7.2.0",
    "react-tooltip": "4.2.15",
    "reactstrap": "8.9.0",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "sass-loader": "10.2.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/polyfill": "7.12.1",
    "@next/bundle-analyzer": "12.0.2",
    "@storybook/addon-actions": "^6.4.0-alpha.20",
    "@storybook/addon-essentials": "^6.4.0-alpha.20",
    "@storybook/addon-links": "^6.4.0-alpha.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.0-alpha.20",
    "@types/cookie": "^0.4.0",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-datepicker": "3.1.1",
    "@types/react-dom": "16.9.6",
    "@types/react-native": "0.63.23",
    "@types/react-redux": "7.1.9",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "3.10.1",
    "@zeit/next-source-maps": "0.0.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-react-native-web": "^0.17.1",
    "chromedriver": "95.0.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "next-fonts": "1.5.1",
    "next-images": "1.8.2",
    "nightwatch": "1.6.3",
    "patch-package": "6.2.2",
    "postcss": "^8.3.0",
    "prettier": "^2.3.2",
    "redux-devtools-extension": "2.13.9",
    "sass": "1.32.13",
    "suppress-exit-code": "^1.0.0",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{js,tsx,ts}": "suppress-exit-code eslint --fix --quiet"
  }
}
