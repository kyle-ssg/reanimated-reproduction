{
  "name": "kimble-broker",
  "version": "0.1.1",
  "description": "Node Express Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack:prod && node ./server",
    "dev": "nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "test": "nodemon --exec mocha ./server/tests/",
    "webpack:dev": "node & ./node_modules/.bin/webpack-dashboard -m -- webpack-dev-server --watch --hot --progress --inline --config ./config/webpack.config.dev.js",
    "webpack:prod": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "2.2.5",
    "colors": "1.1.2",
    "firebase": "3.2.0",
    "flux-react-dispatcher": "1.2.5",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-watch": "4.3.11",
    "ps-node": "0.1.4",
    "rc-switch": "1.4.2",
    "react-maskedinput": "3.3.4",
    "react-router": "2.6.0",
    "react-select": "1.0.0-rc.3",
    "react-slider": "0.7.0",
    "react-sortable-hoc": "0.0.6",
    "react-tooltip": "3.1.2",
    "react-virtualized": "^7.0.0",
    "react-virtualized-infinite-scroll": "^0.0.4",
    "simple-firebase-auth": "^0.0.6",
    "ssg-react": "15.0.0",
    "ssg-node-express": "4.14.1",
    "ssg-util": "0.0.1",
    "ssg-webpack": "2.2.1"
  }
}
