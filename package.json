{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=true next build ",
    "dev": "next",
    "env_script": "sh .bin/env.sh",
    "preinstall": "npm run env_script",
    "postinstall": "patch-package && husky install .bin/.husky",
    "build": "next build",
    "update_version": "export CI_COMMIT_REF_NAME=\"$(git rev-parse --abbrev-ref HEAD)\"\ncd ./mobile && npm run set_native_version && cd .. ",
    "static": "npm run build && npm run export",
    "export": "next export ",
    "start": "cross-env NODE_ENV=production cross-env NODE_ENV=production next start",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint . --ext .tsx,.ts --fix --quiet",
    "typecheck": "tsc",
    "test:e2e:bundle": "cross-env E2E=1 npm run build",
    "test:e2e:local": "ts-node-transpile-only ./.e2e/index.cafe",
    "test:e2e:prod": "npm run test:e2e:bundle && cross-env NODE_ENV=production E2E=true npm run test:e2e:local",
    "storybook": "start-storybook -p 4000 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@reduxjs/toolkit": "^1.7.2",
    "autoprefixer": "^10.4.4",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "dayjs": "^1.10.7",
    "formik": "2.2.9",
    "lodash-es": "^4.17.21",
    "next": "^12.1.0",
    "next-plugin-preact": "^3.0.6",
    "next-redux-wrapper": "7.0.5",
    "nookies": "^2.5.2",
    "nprogress": "^0.2.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.3",
    "preact": "^10.6.6",
    "preact-render-to-string": "^5.1.20",
    "rc-switch": "3.2.2",
    "react": "npm:@preact/compat@^17.0.3",
    "react-datepicker": "4.6.0",
    "react-dom": "npm:@preact/compat@^17.0.3",
    "react-localization": "1.0.17",
    "react-redux": "7.2.6",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.2.0",
    "react-tooltip": "4.2.21",
    "reactstrap": "9.0.1",
    "redux": "4.1.2",
    "redux-persist": "^6.0.0",
    "ssgrtk": "^0.0.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/polyfill": "7.12.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@next/bundle-analyzer": "12.0.10",
    "@slack/web-api": "^6.7.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.19",
    "@tsconfig/next": "^1.0.2",
    "@types/cookie": "^0.4.1",
    "@types/express": "^4.17.13",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "17.0.17",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.39",
    "@types/react-datepicker": "4.3.4",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.9",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "next-transpile-modules": "^9.0.0",
    "patch-package": "6.4.7",
    "postcss": "^8.3.0",
    "prettier": "^2.5.1",
    "sass": "1.32.13",
    "sass-loader": "10.2.0",
    "storybook-addon-pseudo-states": "^1.9.0",
    "storybook-css-modules-preset": "^1.1.1",
    "suppress-exit-code": "^1.0.0",
    "testcafe": "^1.18.3",
    "ts-node": "^10.5.0",
    "typescript": "4.5.5",
    "vercel": "^24.2.3"
  },
  "lint-staged": {
    "*.{js,tsx,ts}": "suppress-exit-code eslint --fix --quiet"
  }
}
