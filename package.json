{
  "name": "ssg-frontend-boilerplate",
  "version": "0.1.1",
  "description": "Node Express Boilerplate",
  "main": "index.js",
  "scripts": {
    "env_prod": "ENV=production node ./bin/env.js",
    "env_dev": "ENV=dev node ./bin/env.js",
    "start": "npm run bundle && NODE_ENV=production node ./server",
    "dev": "nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js"
  },
  "dependencies": {
    "animate.css": "3.5.2",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "firebase": "4.12.1",
    "flux-react-dispatcher": "^1.2.5",
    "font-awesome": "4.7.0",
    "html-webpack-plugin": "3.1.0",
    "jquery": "3.3.1",
    "lodash-webpack-plugin": "0.11.2",
    "promise-polyfill": "7.1.1",
    "prop-types": "15.6.1",
    "rc-switch": "1.6.0",
    "react-maskedinput": "4.0.1",
    "react-tooltip": "3.4.0",
    "react-virtualized": "9.7.5",
    "simple-firebase-auth": "0.0.8",
    "ssg-node-express": "4.16.3",
    "ssg-react": "16.3.1",
    "ssg-util": "0.0.2",
    "ssg-webpack": "4.4.1",
    "tether": "1.4.3",
    "webpack-hot-client": "2.2.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "webpack-cli": "2.0.13"
  }
}
