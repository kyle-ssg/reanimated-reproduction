{
  "name": "shared-react",
  "version": "0.1.0",
  "description": "An example cross platform application",
  "main": "index.js",
  "repository": "https://github.com/SolidStateGroup/shared-react",
  "license": "MIT",
  "scripts": {
    "analyse": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.analyse.js",
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js",
    "dev": "npm run lint:watch & npm run env_dev && node_modules/.bin/nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "env_dev": "cross-env ENV=dev node ./bin/env.js",
    "env_prod": "cross-env ENV=prod node ./bin/env.js",
    "lint": "eslint 'web/**/*.js' 'tests/**/*.js' 'common/**/*.js'",
    "lint:fix": "eslint --fix 'web/**/*.js' 'common/**/*.js'",
    "lint:watch": "esw -w --changed 'web/**/*.js'",
    "start": "npm run env_prod && npm run bundle && NODE_ENV=production node ./server",
    "test:e2e": "cross-env E2E=true nightwatch ./tests/e2e/index.e2e.js",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:unit:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --watch --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:pact": "cross-env NODE_ENV=pactTest mocha --opts ./tests/mocha.opts \"**/*.pact.js\"",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js",
    "test:coverage": "istanbul cover ./web/main.js"
  },
  "engines": {
    "node": "9.10.1",
    "npm": "6.4.1"
  },
  "dependencies": {
    "bootstrap": "4.1.3",
    "chromedriver": "2.42.0",
    "classnames": "2.2.6",
    "colors": "1.3.2",
    "cross-env": "5.2.0",
    "fbjs": "0.8.17",
    "flux-react-dispatcher": "1.2.5",
    "formik": "1.3.1",
    "ionicons": "4.4.4",
    "js-base64": "2.4.9",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "node-fetch": "2.2.0",
    "node-sass": "4.9.3",
    "node-slack-upload": "1.2.1",
    "polyfill-react-native": "1.0.5",
