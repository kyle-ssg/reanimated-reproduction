{
  "name": "mobile",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "reset-cache": "react-native start --reset-cache",
    "android": "react-native run-android --variant=uatDebug --appIdSuffix 'uat'",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:release": "react-native run-android --variant=prodRelease",
    "android:release:uat": "react-native run-android --variant=uatRelease --appIdSuffix 'uat'",
    "android:bundle": "cd ./android && ./gradlew bundleProdRelease && open ./app/build/outputs/bundle/prodRelease && cd ..",
    "android:bundle:uat": "cd ./android && ./gradlew bundleUatRelease && open ./app/build/outputs/bundle/uatRelease && cd ..",
    "android:apk": "cd ./android && ./gradlew assembleProdRelease && open ./app/build/outputs/apk/prod/release && cd ..",
    "android:apk:uat": "cd ./android && ./gradlew assembleUatRelease && open ./app/build/outputs/apk/uat/release && cd ..",
    "android:hash:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore | openssl sha1 -binary | openssl base64\n",
    "android:keystore": "keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "android:menu": "adb shell input keyevent 82",
    "upload:sourcemaps": "sh ./.bin/upload-sourcemaps.sh",
    "env_script": "sh .bin/env.sh",
    "lint": "eslint .. || exit 0",
    "lint:fix": "eslint .. --fix || exit 0",
    "postinstall": ".bin/postinstall.sh && patch-package",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:android:build": "detox build --configuration android28",
    "test:android:all": "npm run test:android28 & npm run test:android30",
    "test:ios:all": "npm run test:ios & npm run test:ios12 & npm run test:ipad",
    "test:ios:build": "detox build --configuration ios12",
    "test:ios:buildProd": "detox build --configuration iosProd",
    "test:android28": "npx e2e-proxy & detox test --configuration android28 --workers 4 --jest-report-specs",
    "test:android30": "npx e2e-proxy & detox test --configuration android30 --workers 4 --jest-report-specs",
    "test:ios": "npx e2e-proxy & detox test --configuration ios --workers 4 --jest-report-specs",
    "test:clean": "detox rebuild-framework-cache",
    "test:iosProd": "npx e2e-proxy & detox test --configuration iosProd --workers 4 --jest-report-specs",
    "test:ios12": "npx e2e-proxy & detox test --configuration ios12 --workers 4 --jest-report-specs",
    "test:ipad": "npx e2e-proxy & detox test --configuration ipad --workers 4 --jest-report-specs",
    "preinstall": "npm run env_script"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "3.4.7",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^4.7.0",
    "@react-native-firebase/analytics": "^8.0.1",
    "@react-native-firebase/app": "^9.0.0",
    "@react-native-firebase/messaging": "^8.0.1",
    "@react-navigation/bottom-tabs": "^5.10.6",
    "@react-navigation/native": "^5.8.6",
    "@react-navigation/stack": "^5.12.3",
    "@types/redux-persist": "^4.3.1",
    "amazon-cognito-identity-js": "^4.5.11",
    "aws-amplify": "^3.3.20",
    "aws-amplify-react-native": "^4.3.1",
    "detox-getprops": "^0.1.2",
    "e2e-proxy": "^1.7.0",
    "formik": "^2.2.1",
    "hermes-engine": "0.7.2",
    "immer": "^7.0.5",
    "lodash": "^4.17.21",
    "lottie-ios": "^3.1.8",
    "lottie-react-native": "^4.0.2",
    "memoize-one": "^5.1.1",
    "moment": "^2.27.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-auto-height-image": "^3.2.1",
    "react-native-background-timer": "^2.3.1",
    "react-native-bottomsheet": "^2.2.0",
    "react-native-code-push": "^6.3.0",
    "react-native-device-info": "^5.6.2",
    "react-native-fast-image": "^8.3.2",
    "react-native-flipper": "0.90.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-globals": "^0.62.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-launch-arguments": "^2.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.1.6",
    "react-native-mmkv-storage": "^0.3.5",
    "react-native-modal-datetime-picker": "^8.9.3",
    "react-native-performance-monitor": "^1.2.1",
    "react-native-reanimated": "2.1.0",
    "react-native-redash": "^15.11.1",
    "react-native-render-html": "^4.2.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.17.1",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.15.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^10.10.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-flipper": "1.4.2",
    "redux-middleware-flipper": "^1.0.7",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.0.0-rc.14",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@storybook/addons": "^6.0.0-rc.14",
    "@storybook/react-native": "^5.3.19",
    "@testing-library/react-native": "^7.2.0",
    "@types/detox": "^17.14.0",
    "@types/jasmine": "^3.6.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.159",
    "@types/react": "^16.9.44",
    "@types/react-native": "^0.63.1",
    "@types/react-native-auth0": "^2.5.4",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.0.1",
    "detox": "^18.15.0",
    "eslint": "7.14.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "hermes-engine": "^0.5.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "node-fetch": "^2.6.0",
    "patch-package": "^6.2.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "17.0.1",
    "storybook-addon-smart-knobs": "https://github.com/storybookjs/addon-smart-knobs#master",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": ["./setupTests.ts"]
  }
}
