{
  "name": "mobile",
  "version": "1.18.2",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=uatDebug --appIdSuffix 'uat'",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:release": "react-native run-android --variant=prodRelease",
    "android:release:uat": "react-native run-android --variant=uatRelease --appIdSuffix 'uat'",
    "android:bundle": "cd ./android && ./gradlew bundleProdRelease && open ./app/build/outputs/bundle/prodRelease && cd ..",
    "android:bundle:uat": "cd ./android && ./gradlew bundleUatRelease && open ./app/build/outputs/bundle/uatRelease && cd ..",
    "android:apk": "cd ./android && ./gradlew assembleProdRelease && open ./app/build/outputs/apk/prod/release && cd ..",
    "android:apk:uat": "cd ./android && ./gradlew assembleUatRelease && open ./app/build/outputs/apk/uat/release && cd ..",
    "android:hash:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore | openssl sha1 -binary | openssl base64\n",
    "android:keystore": "keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "android:menu": "adb shell input keyevent 82",
    "env_script": "sh .bin/env.sh",
    "lint": "cd .. && npm run lint || exit 0",
    "lint:fix": "cd .. && npm run lint:fix || exit 0",
    "postinstall": ".bin/postinstall.sh && patch-package",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:updateSnapshots": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:android:build": "detox build --configuration android28",
    "test:android:all": "npm run test:android28 & npm run test:android30",
    "test:ios:all": "npm run test:ios & npm run test:ios12 & npm run test:ipad",
    "test:ios:build": "detox build --configuration ios12",
    "test:ios:buildProd": "detox build --configuration iosProd",
    "test:android28": "npx e2e-proxy & detox test --configuration android28 --workers 4 --jest-report-specs",
    "test:android30": "npx e2e-proxy & detox test --configuration android30 --workers 4 --jest-report-specs",
    "test:ios": "npx e2e-proxy & detox test --configuration ios --workers 4 --jest-report-specs",
    "test:clean": "detox rebuild-framework-cache",
    "test:iosProd": "npx e2e-proxy & detox test --configuration iosProd --workers 4 --jest-report-specs",
    "test:ios12": "npx e2e-proxy & detox test --configuration ios12 --workers 4 --jest-report-specs",
    "test:ipad": "npx e2e-proxy & detox test --configuration ipad --workers 4 --jest-report-specs",
    "typecheck": "tsc",
    "preinstall": "npm run env_script",
    "set_native_version": "node ./.bin/set-native-version.js",
    "bump_native_version": "./.bin/bump-native-version.sh $VERSION",
    "storybook": "npm run storybook:get && start-storybook",
    "storybook:get": "npx sb-rn-watcher --config-path=./.storybook"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.1.5",
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-native-community/datetimepicker": "5.1.0",
    "@react-native-community/hooks": "2.8.1",
    "@react-native-community/netinfo": "8.0.0",
    "@react-native-firebase/analytics": "14.3.2",
    "@react-native-firebase/app": "14.3.2",
    "@react-native-firebase/messaging": "14.3.2",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "dayjs": "^1.10.7",
    "detox-getprops": "^0.1.2",
    "e2e-proxy": "^1.7.0",
    "formik": "^2.2.9",
    "hermes-engine": "0.9.0",
    "immer": "^9.0.12",
    "lodash-es": "^4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "5.0.1",
    "next-redux-wrapper": "^7.0.5",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-code-push": "^7.0.4",
    "react-native-device-info": "^8.4.9",
    "react-native-flipper": "0.131.1",
    "react-native-gesture-handler": "1.10.3",
    "react-native-globals": "^0.62.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-launch-arguments": "git://github.com/Flagsmith/react-native-launch-arguments.git#dist",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "2.2.0",
    "react-native-mmkv-storage": "^0.6.12",
    "react-native-modal-datetime-picker": "13.0.1",
    "react-native-reanimated": "2.4.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.1",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-native-webview": "^11.17.2",
    "react-redux": "^7.2.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "7.16.11",
    "@babel/register": "7.17.0",
    "@babel/runtime": "^7.17.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-ondevice-controls": "^6.0.1-alpha.7",
    "@storybook/addons": "^6.4.19",
    "@storybook/react-native": "^6.0.1-alpha.7",
    "@storybook/react-native-server": "^6.0.0-alpha.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.66.15",
    "@types/react-redux": "^7.1.22",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/parser": "^5.11.0",
    "appcenter-cli": "^2.10.8",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "3.21.0",
    "detox": "^18.15.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "lint-staged": "12.2.2",
    "metro-react-native-babel-preset": "0.68.0",
    "node-fetch": "3.2.0",
    "patch-package": "6.4.7",
    "react-dom": "17.0.2",
    "react-native-set-version": "^1.1.2",
    "react-test-renderer": "17.0.2",
    "redux-flipper-colorized": "^2.0.5",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
