{
  "name": "janus-b2c",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.com:racing-post/janus/janus-frontend-common.git"
  },
  "scripts": {
    "start": "react-native start",
    "android": "cd ./android && ./gradlew installProdDebug",
    "android:uat": "cd ./android && ./gradlew installUatDebug",
    "android:bundle": "cd ./android && ./gradlew assembleProdRelease",
    "android:bundle:uat": "cd ./android && ./gradlew assembleUatRelease",
    "android:hash:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore | openssl sha1 -binary | openssl base64\n",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "hooks": "git config core.hooksPath ./hooks",
    "hooks:mac": "cp ./hooks/pre-commit ./.git/hooks/pre-commit && chmod +x ./.git/hooks/pre-commit",
    "postinstall": "npm run hooks",
    "storybook": "start-storybook -p 7007",
    "test": "jest ./storybook/storybook.test.js",
    "e2e:android": "wdio ./tests/e2e/config/wdio.android.conf.js",
    "e2e:ios": "wdio ./tests/e2e/config/wdio.android.conf.js",
    "e2e:android:bs": "wdio ./tests/e2e/config/browserstack/wdio.android.rdc.conf.js",
    "e2e:ios:bs": "wdio ./tests/e2e/config/browserstack/wdio.ios.rdc.conf.js"
  },
  "rnpm": {
    "assets": [
      "./app/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
