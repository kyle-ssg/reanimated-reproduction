{
  "name": "ssg-mobile-boilerplate",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=uatDebug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:release": "react-native run-android --variant=prodRelease",
    "android:release:uat": "react-native run-android --variant=uatRelease",
    "android:bundle": "cd ./android && ./gradlew bundleProdRelease && open ./app/build/outputs/bundle/prodRelease && cd ..",
    "android:bundle:uat": "cd ./android && ./gradlew bundleUatRelease && open ./app/build/outputs/bundle/uatRelease && cd ..",
    "android:apk": "cd ./android && ./gradlew assembleProdRelease && open ./app/build/outputs/apk/prod/release && cd ..",
    "android:apk:uat": "cd ./android && ./gradlew assembleUatRelease && open ./app/build/outputs/apk/uat/release && cd ..",
    "android:hash:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore | openssl sha1 -binary | openssl base64\n",
    "android:keystore": "keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "env_script": "sh .bin/env.sh",
    "lint": "eslint . || exit 0",
    "lint:fix": "eslint . --fix || exit 0",
    "postinstall": ".bin/postinstall.sh && patch-package",
    "preinstall": "npm run env_script"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.1.5",
    "@react-native-firebase/analytics": "^7.3.1",
    "@react-native-firebase/app": "^8.2.0",
    "@react-native-firebase/messaging": "^7.4.2",
    "@react-navigation/native": "^5.7.1",
    "formik": "^2.1.5",
    "immer": "^7.0.5",
    "lottie-ios": "^3.1.8",
    "lottie-react-native": "^3.4.0",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-auto-height-image": "^3.2.1",
    "react-native-background-timer": "^2.3.1",
    "react-native-bottomsheet": "^2.2.0",
    "react-native-code-push": "^6.3.0",
    "react-native-device-info": "^5.6.2",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-globals": "^0.62.0",
    "react-native-localization": "^2.1.6",
    "react-native-mmkv-storage": "^0.3.5",
    "react-native-modal-datetime-picker": "^8.7.1",
    "react-native-performance-monitor": "^1.2.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-redash": "^14.2.3",
    "react-native-render-html": "^4.2.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-shared-element": "^0.7.0",
    "react-native-static-safe-area-insets": "^2.1.1",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^10.3.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-promise-middleware": "^6.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.0.0-rc.14",
    "@storybook/addon-knobs": "^6.0.0-rc.14",
    "@storybook/addon-links": "^6.0.0-rc.14",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@storybook/addons": "^6.0.0-rc.14",
    "@storybook/react-native": "^5.3.19",
    "@types/react-native": "^0.63.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.0.1",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-react": "^7.20.3",
    "install": "^0.13.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.60.0",
    "npm": "^6.14.6",
    "patch-package": "^6.2.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "16.13.1",
    "storybook-addon-smart-knobs": "https://github.com/storybookjs/addon-smart-knobs#master",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native"
  }
}
