{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios & npm run watch",
    "android": "react-native run-android & npm run watch",
    "androidDeploy": "cd ./android && ./gradlew assembleRelease && ./gradlew installRelease && cd ../",
    "apk": "cd ./android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release && cd ../",
    "postinstall": "cp -r ../common ./common-mobile",
    "keystore": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "test": "detox test",
    "watch": "cd ./bin && sh ./common-watch.sh"
  },
  "dependencies": {
    "events": "^1.1.1",
    "flux-react-dispatcher": "^1.2.5",
    "inputmask-core": "^2.2.0",
    "js-base64": "^2.4.8",
    "lottie-react-native": "^2.3.2",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-native-animatable": "^1.3.0",
    "react-native-bottomsheet": "^1.8.1",
    "react-native-branch": "^2.3.0",
    "react-native-device-info": "^0.21.5",
    "react-native-fabric": "^0.5.1",
    "react-native-fabric-crashlytics": "^0.1.8",
    "react-native-firebase": "^4.3.8",
    "react-native-globals": "^0.50.0",
    "react-native-interactable": "git+https://github.com/gabeboning/react-native-interactable.git#master",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-navigation": "^1.1.471",
    "react-native-svg": "^6.5.2",
    "react-native-vector-icons": "^4.6.0",
    "react-native-view-overflow": "0.0.3",
    "ssg-util": "0.0.2"
  },
  "devDependencies": {
    "babel-jest": "23.4.2",
    "@babel/core": "7.0.0-beta.47",
    "babel-preset-react-native": "^5",
    "react-test-renderer": "16.4.1",
    "detox": "^7.3.3",
    "mocha": "^5.0.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/boilerplate.app/",
        "build": "xcodebuild -workspace ios/boilerplate.xcworkspace -scheme boilerplate -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8 Plus"
      }
    }
  }
}
