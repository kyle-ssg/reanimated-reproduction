{
  "name": "ssg-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "cp -R ../src/js/common ./common",
    "mobile:watch": "node ../bin/commonWatch.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios & npm run mobile:watch",
    "android": "react-native run-android & npm run mobile:watch",
    "androidDeploy": "cd ./android && ./gradlew assembleRelease && ./gradlew installRelease && cd ../",
    "keystore": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000"
  },
  "dependencies": {
    "inputmask-core": "^2.2.0",
    "lottie-react-native": "https://github.com/kyle-ssg/lottie-react-native#master",
    "react-native-actionsheet": "^2.1.0",
    "react-native-animatable": "^1.2.3",
    "react-native-autolink": "^1.0.0",
    "react-native-bottomsheet": "1.6.3",
    "react-native-branch": "^2.0.0",
    "react-native-cached-image": "^1.3.4",
    "react-native-camera": "^0.8.0",
    "react-native-charts-wrapper": "^0.2.10",
    "react-native-contacts": "^0.9.1",
    "react-native-datepicker": "^1.4.4",
    "react-native-device-info": "^0.11.0",
    "react-native-facebook-login": "github:patw0929/react-native-facebook-login#e7c819d90700d908d6f2e600c8bb95874aaa9edd",
    "react-native-fcm": "9.2.0",
    "react-native-fetch-blob": "^0.10.2",
    "react-native-firebase-auth": "^1.0.1",
    "react-native-fs": "^2.3.2",
    "react-native-globals": "0.44.0",
    "react-native-google-analytics": "^1.3.2",
    "react-native-google-sign-in": "0.0.16",
    "react-native-image-crop-picker": "0.16.1",
    "react-native-image-resizer": "1.0.0",
    "react-native-interactable": "0.1.3",
    "react-native-invertible-scroll-view": "^1.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-navigation": "1.1.228",
    "react-native-scrollable-tab-view": "^0.7.1",
    "react-native-session-length": "0.0.1",
    "react-native-share": "^1.0.20",
    "react-native-svg": "5.4.1",
    "react-native-vector-icons": "^4.3.0",
    "socket.io-client": "^1.4.5",
    "ssg-react-native": "^0.48.2",
    "ssg-util": "0.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
