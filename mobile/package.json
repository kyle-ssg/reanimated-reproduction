{
  "name": "ssg-mobile-boilerplate",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://git.solidstategroup.com/solidstategroup/ssg-frontend-boilerplate"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=prodDebug",
    "android:uat": "react-native run-android --variant=uatDebug",
    "android:release": "react-native run-android --variant=prodRelease",
    "android:release:uat": "react-native run-android --variant=uatRelease",
    "android:bundle": "cd ./android && ./gradlew bundleProdRelease && open ./app/build/outputs/bundle/prodRelease && cd ..",
    "android:bundle:uat": "cd ./android && ./gradlew bundleUatRelease && open ./app/build/outputs/bundle/uatRelease && cd ..",
    "android:apk": "cd ./android && ./gradlew assembleProdRelease && open ./app/build/outputs/apk/prod/release && cd ..",
    "android:apk:uat": "cd ./android && ./gradlew assembleUatRelease && open ./app/build/outputs/apk/uat/release && cd ..",
    "android:hash:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore | openssl sha1 -binary | openssl base64\n",
    "dependencies": "depcruise --exclude \"^node_modules\" --output-type dot . | dot -T svg > dependencygraph.svg\n\n",
    "lint": "eslint . || exit 0",
    "lint:fix": "eslint . --fix || exit 0",
    "postinstall": "./postinstall.sh",
    "storybook": "start-storybook -p 7007",
    "test:ui": "jest ./storybook/storybook.test.js"
  },
  "rnpm": {
    "assets": [
      "./app/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "3.0.2",
    "@react-native-community/netinfo": "^4.1.5",
    "@react-native-firebase/analytics": "^6.0.3",
    "@react-native-firebase/app": "^6.0.3",
    "formik": "^2.1.4",
    "fs-extra": "^8.1.0",
    "immer": "^7.0.5",
    "isomorphic-unfetch": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "lottie-ios": "^3.0.3",
    "lottie-react-native": "^3.1.0",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "0.63.1",
    "react-native-animatable": "^1.3.2",
    "react-native-auto-height-image": "^1.1.3",
    "react-native-background-timer": "^2.1.1",
    "react-native-bottomsheet": "^2.0.1",
    "react-native-bullet-train": "^0.0.82",
    "react-native-code-push": "^6.1.1",
    "react-native-device-info": "5.6.1",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-globals": "^0.60.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.1.5",
    "react-native-mmkv-storage": "^0.3.5",
    "react-native-modal-datetime-picker": "^7.6.0",
    "react-native-navigation": "^6.3.2",
    "react-native-navigation-drawer-extension": "^2.0.2",
    "react-native-performance-monitor": "^1.0.13",
    "react-native-reanimated": "^1.2.0",
    "react-native-render-html": "^4.1.2",
    "react-native-svg": "9.7.1",
    "react-native-swiper": "git+https://github.com/kyle-ssg/react-native-swiper#skip-initial-render",
    "react-native-tab-view": "^2.11.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "8.2.1",
    "react-redux": "5.1.1",
    "redux": "3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "6.1.1",
    "redux-saga": "1.0.5",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "5.3.0-beta.3",
    "@storybook/addon-knobs": "5.3.0-beta.3",
    "@storybook/addon-links": "5.3.0-beta.3",
    "@storybook/addon-ondevice-actions": "5.3.0-beta.3",
    "@storybook/addon-ondevice-backgrounds": "5.3.0-beta.3",
    "@storybook/addon-ondevice-knobs": "5.3.0-beta.3",
    "@storybook/addon-storyshots": "5.3.0-beta.3",
    "@storybook/addons": "5.3.0-beta.3",
    "@storybook/react-native": "5.3.0-beta.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-docgen": "^3.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.0.1",
    "dependency-cruiser": "^9.8.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-watch": "^6.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^1.15.2",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.13.1",
    "storybook-addon-smart-knobs": "git+https://github.com/kyle-ssg/addon-smart-knobs#master"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
