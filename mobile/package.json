{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "mobile:watch": "node ../bin/commonWatch.js",
    "fix": "node node_modules/eslint-watch/bin/esw -w --color --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios & npm run mobile:watch",
    "android": "react-native run-android & npm run mobile:watch",
    "androidDeploy": "cd ./android && ./gradlew assembleRelease && ./gradlew installRelease && cd ../",
    "postinstall": "mkdir -p common & node ../bin/patchMobile.js",
    "keystore": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000"
  },
  "dependencies": {
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "firebase": "^3.6.6",
    "flux-react-dispatcher": "^1.2.5",
    "inputmask-core": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-branch": "^1.0.1",
    "react-native-fabric": "^0.4.1",
    "react-native-fabric-crashlytics": "https://github.com/jondot/react-native-fabric-crashlytics#master",
    "react-native-fabric-digits": "^1.0.23",
    "react-native-facebook-login": "https://github.com/magus/react-native-facebook-login",
    "react-native-fcm": "^6.2.3",
    "react-native-firebase-auth": "https://github.com/SolidStateGroup/react-native-firebase-auth#react-native-google-sign-in",
    "react-native-fs": "^2.3.2",
    "react-native-google-analytics-bridge": "^5.0.0",
    "react-native-google-sign-in": "^0.0.8",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-native-navigation": "^1.1.76",
    "react-native-scrollable-tab-view": "^0.7.1",
    "react-native-side-menu": "^0.20.1",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.9",
    "react-router-native": "^2.0.0-beta.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "20.0.0",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
