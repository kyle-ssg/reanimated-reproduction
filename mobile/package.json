{
  "name": "ssg-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "cp -R ../src/common ./ && rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "mobile:watch": "node ../bin/commonWatch.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios & npm run mobile:watch",
    "android": "react-native run-android & npm run mobile:watch",
    "androidDeploy": "cd ./android && ./gradlew assembleRelease && ./gradlew installRelease && cd ../",
    "keystore": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000"
  },
  "dependencies": {
    "events": "^1.1.1",
    "flux-react-dispatcher": "^1.2.5",
    "inputmask-core": "^2.2.0",
    "lottie-react-native": "^2.3.2",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-animatable": "^1.2.3",
    "react-native-bottomsheet": "1.6.3",
    "react-native-branch": "^2.0.0",
    "react-native-cached-image": "^1.4.3",
    "react-native-camera": "^0.12.0",
    "react-native-contacts": "^0.9.1",
    "react-native-datepicker": "^1.4.4",
    "react-native-device-info": "^0.11.0",
    "react-native-fabric": "^0.5.1",
    "react-native-fabric-crashlytics": "^0.1.8",
    "react-native-facebook-login": "1.6.1",
    "react-native-fetch-blob": "^0.10.2",
    "react-native-firebase": "^3.1.1",
    "react-native-fs": "^2.8.4",
    "react-native-globals": "0.50.0",
    "react-native-google-sign-in": "^1.0.16",
    "react-native-image-crop-picker": "0.16.1",
    "react-native-image-resizer": "^1.0.0",
    "react-native-interactable": "https://github.com/ssg-luke/react-native-interactable#bugfix/drag-toss-android",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-navigation": "1.1.402",
    "react-native-rate": "^1.0.8",
    "react-native-scrollable-tab-view": "^0.7.1",
    "react-native-segmented-control-tab": "^3.2.1",
    "react-native-session-length": "0.0.1",
    "react-native-svg": "5.4.1",
    "react-native-twitter": "https://github.com/kyle-ssg/react-native-twitter#master",
    "react-native-vector-icons": "4.5.0",
    "ssg-util": "0.0.1"
  },
  "devDependencies": {
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
