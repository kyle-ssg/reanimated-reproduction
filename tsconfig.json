{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "components/": [
        "./components/*"
      ],
      "common/": [
        "./common/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
    "mobile"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types/global.d.ts",
    "./mobile/src/global.d.ts",
    "components/extras/CookiePolicy.js",
    "components/base/forms/Tabs.js",
    "components/extras/PanelSearch.js",
    "common/project.js",
    "common/providers/WithSetValue.js",
    "common/providers/_hocTemplate.js",
    "components/extras/withGracecaptcher.ts",
    "mobile/app/components/base/forms/Select.tsx"
  ]
}
